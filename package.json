    "@shopify/app-bridge-utils": "^2.0.26",
    "@shopify/polaris": "^9.14.1",
    "@shopify/shopify-api": "^3.0.0",
    "@vitejs/plugin-react": "1.3.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.5.0",
    "liquidjs": "^9.37.0",
    "raw-body": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-static": "^1.14.1",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13",
    "vite": "^2.9.8"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
